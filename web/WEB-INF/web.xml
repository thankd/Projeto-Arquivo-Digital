<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>logar</servlet-name>
        <servlet-class>br.edu.ifrn.servlet.logar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarUsuario</servlet-name>
        <servlet-class>Servlets.CadastrarUsuario</servlet-class>
    </servlet>
    <servlet>
    <servlet-name>GerenciarProcesso</servlet-name>
    <servlet-class>Servlets.GerenciarProcesso</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>GerenciarProcesso</servlet-name>
    <url-pattern>/GerenciarProcesso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logar</servlet-name>
        <url-pattern>/logar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarUsuario</servlet-name>
        <url-pattern>/CadastrarUsuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
